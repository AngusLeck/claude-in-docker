# Copy this to .env and fill in your credentials

# Git user configuration
GIT_USER_NAME=Your Name
GIT_USER_EMAIL=your.email@example.com

# GitHub Personal Access Token (for repo access)  
GITHUB_TOKEN=ghp_your_github_token_here

# Claude credentials (from macOS keychain or ~/.claude/.credentials.json)
CLAUDE_CREDENTIALS={"claudeAiOauth":{"accessToken":"your_access_token","refreshToken":"your_refresh_token","expiresAt":1234567890,"scopes":["user:inference","user:profile"],"subscriptionType":"max"}}